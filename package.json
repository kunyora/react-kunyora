{
  "name": "react-kunyora",
  "version": "0.1.0",
  "description":
    "A reactJs binding to kunyora that exposes some declarative apis used in performing restful request and improving speed",
  "main": "dist/react-kunyora.js",
  "scripts": {
    "clean": "rimraf lib dist es",
    "test": "cross-env NODE_ENV=test && jest",
    "watch:test": "cross-env NODE_ENV=test && jest --watchAll",
    "format": "prettier --config ./.prettierrc --write \"{src,__tests__,__customMockData__}/**/*.js\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"{src,__tests__,__customMockData__}/**/*.js\"",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/react-kunyora.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/react-kunyora.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/react-kunyora.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/react-kunyora.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepublish": "yarn run test && yarn run build"
  },
  "keywords": [
    "kunyora",
    "react",
    "disposable",
    "react-native",
    "request",
    "api",
    "restful"
  ],
  "author": "Anifowoshe Gbenga David <daveanifowoshe@ymail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.6",
    "enzyme": "^3.3.0",
    "jest": "^22.4.2",
    "prettier": "^1.13.3",
    "react": "^16.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
  },
  "dependencies": {
    "create-react-context": "^0.1.6",
    "kunyora": "0.1.1",
    "prop-types": "^15.6.0"
  },
  "bugs": {
    "url": "https://github.com/kunyora/react-kunyora/issues"
  },
  "homepage": {
    "url": "https://kunyora.github.io/kunyora/query_component.html"
  },
  "unpkg": "dist/react-kunyora.js"
}
